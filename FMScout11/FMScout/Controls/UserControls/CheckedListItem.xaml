<UserControl x:Class="FMScout.UserControls.CheckedListItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="Auto" Width="Auto">
    <CheckBox x:Name="MyCheckBox" Content="{Binding CheckBoxContent}" IsChecked="{Binding isChecked}" Style="{DynamicResource CheckListBox}"/>
</UserControl>
